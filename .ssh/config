host liu
    HostName ssh.liu.se
    User mikhe33
    Port 22
host only-da-proxy
    HostName only-da
    User mikhe33
    Port 22
    ProxyCommand ssh liu -W %h:%p
    RequestTTY yes
    RemoteCommand scl enable devtoolset-7 zsh
