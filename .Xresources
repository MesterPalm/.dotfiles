!
! DPI settings. It appears as GNOME does not honor this setting and overwrites
! the DPI setting after .Xresources have been loaded. Oh well, still usefull for
! other DE/WM.
!
Xft.dpi:	89

! Font settins.
*.alpha: 150
*.font:     xft:MesloLGS NF:style=Regular:size=12, \
            xft:Hack:style=Regular:size=12, \
            xft:WenQuauYi Zen Hei Mono:style=Regular:size=12, \
            xft:DejaVu Sans:size=12, \
            xft:PowerlineSymbols:style=Medium:size=12, \

! Cursor settings.
Xcursor.size:   32
Xcursor.theme:  redglass
!*.cursorColor:  #c5c8c6

! Emacs settings.
emacs.font: Monospace 12

! Urxvt settings.
URxvt.letterSpace:          0
URxvt.scrollBar:            false
URxvt*transparent:          false
URxvt*shading:              60

